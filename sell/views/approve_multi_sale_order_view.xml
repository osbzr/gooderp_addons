							<button string="取消" class="oe_link" special="cancel"/>
						</footer>
					</form>
				</field>
		</record>

		<act_window id="approve_multi_sell_order_action"
			name='批量确认销售订单'
			res_model="approve.multi.sell.order"
			src_model="sell.order"
			multi="True"
			view_type="form"
			view_mode="form"
			target="new"
			/>
	</data>
</openerp>
